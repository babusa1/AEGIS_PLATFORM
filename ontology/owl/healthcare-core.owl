<?xml version="1.0"?>
<rdf:RDF xmlns="http://aegis.health/ontology/core#"
     xml:base="http://aegis.health/ontology/core"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:aegis="http://aegis.health/ontology/core#"
     xmlns:fhir="http://hl7.org/fhir/">

    <owl:Ontology rdf:about="http://aegis.health/ontology/core">
        <rdfs:label>AEGIS Healthcare Core Ontology</rdfs:label>
        <rdfs:comment>Core healthcare entities for the AEGIS platform - Patient, Provider, Organization</rdfs:comment>
        <owl:versionInfo>0.1.0</owl:versionInfo>
    </owl:Ontology>

    <!-- ============================================== -->
    <!-- CLASSES - Core Healthcare Entities             -->
    <!-- ============================================== -->

    <!-- Patient -->
    <owl:Class rdf:about="#Patient">
        <rdfs:label>Patient</rdfs:label>
        <rdfs:comment>A person receiving healthcare services</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Person"/>
    </owl:Class>

    <!-- Person (abstract base) -->
    <owl:Class rdf:about="#Person">
        <rdfs:label>Person</rdfs:label>
        <rdfs:comment>A human being - base class for Patient and Provider</rdfs:comment>
    </owl:Class>

    <!-- Provider -->
    <owl:Class rdf:about="#Provider">
        <rdfs:label>Provider</rdfs:label>
        <rdfs:comment>A healthcare provider (physician, nurse, etc.)</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Person"/>
    </owl:Class>

    <!-- Organization -->
    <owl:Class rdf:about="#Organization">
        <rdfs:label>Organization</rdfs:label>
        <rdfs:comment>A healthcare organization (hospital, clinic, payer, etc.)</rdfs:comment>
    </owl:Class>

    <!-- Location -->
    <owl:Class rdf:about="#Location">
        <rdfs:label>Location</rdfs:label>
        <rdfs:comment>A physical location (facility, unit, room, bed)</rdfs:comment>
    </owl:Class>

    <!-- ============================================== -->
    <!-- DATA PROPERTIES - Patient                      -->
    <!-- ============================================== -->

    <owl:DatatypeProperty rdf:about="#mrn">
        <rdfs:label>Medical Record Number</rdfs:label>
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#givenName">
        <rdfs:label>Given Name</rdfs:label>
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#familyName">
        <rdfs:label>Family Name</rdfs:label>
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#birthDate">
        <rdfs:label>Birth Date</rdfs:label>
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#gender">
        <rdfs:label>Gender</rdfs:label>
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Values: male, female, other, unknown</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#ssn">
        <rdfs:label>Social Security Number</rdfs:label>
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#phoneNumber">
        <rdfs:label>Phone Number</rdfs:label>
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#email">
        <rdfs:label>Email Address</rdfs:label>
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#addressLine">
        <rdfs:label>Address Line</rdfs:label>
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#city">
        <rdfs:label>City</rdfs:label>
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#state">
        <rdfs:label>State</rdfs:label>
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#postalCode">
        <rdfs:label>Postal Code</rdfs:label>
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- ============================================== -->
    <!-- DATA PROPERTIES - Provider                     -->
    <!-- ============================================== -->

    <owl:DatatypeProperty rdf:about="#npi">
        <rdfs:label>National Provider Identifier</rdfs:label>
        <rdfs:domain rdf:resource="#Provider"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#specialty">
        <rdfs:label>Specialty</rdfs:label>
        <rdfs:domain rdf:resource="#Provider"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#credentials">
        <rdfs:label>Credentials</rdfs:label>
        <rdfs:domain rdf:resource="#Provider"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>e.g., MD, DO, NP, RN</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- ============================================== -->
    <!-- DATA PROPERTIES - Organization                 -->
    <!-- ============================================== -->

    <owl:DatatypeProperty rdf:about="#organizationName">
        <rdfs:label>Organization Name</rdfs:label>
        <rdfs:domain rdf:resource="#Organization"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#organizationType">
        <rdfs:label>Organization Type</rdfs:label>
        <rdfs:domain rdf:resource="#Organization"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Values: hospital, clinic, payer, pharmacy, lab</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#taxId">
        <rdfs:label>Tax ID (EIN)</rdfs:label>
        <rdfs:domain rdf:resource="#Organization"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- ============================================== -->
    <!-- DATA PROPERTIES - Location                     -->
    <!-- ============================================== -->

    <owl:DatatypeProperty rdf:about="#locationName">
        <rdfs:label>Location Name</rdfs:label>
        <rdfs:domain rdf:resource="#Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#locationType">
        <rdfs:label>Location Type</rdfs:label>
        <rdfs:domain rdf:resource="#Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Values: facility, building, floor, unit, room, bed</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#bedStatus">
        <rdfs:label>Bed Status</rdfs:label>
        <rdfs:domain rdf:resource="#Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Values: available, occupied, cleaning, blocked</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- ============================================== -->
    <!-- DATA PROPERTIES - Common (Multi-tenant)        -->
    <!-- ============================================== -->

    <owl:DatatypeProperty rdf:about="#tenantId">
        <rdfs:label>Tenant ID</rdfs:label>
        <rdfs:comment>Multi-tenant isolation identifier</rdfs:comment>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#createdAt">
        <rdfs:label>Created At</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#updatedAt">
        <rdfs:label>Updated At</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#sourceSystem">
        <rdfs:label>Source System</rdfs:label>
        <rdfs:comment>The system this data originated from (e.g., Epic, Cerner)</rdfs:comment>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#sourceId">
        <rdfs:label>Source ID</rdfs:label>
        <rdfs:comment>The ID in the source system</rdfs:comment>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- ============================================== -->
    <!-- OBJECT PROPERTIES - Relationships              -->
    <!-- ============================================== -->

    <owl:ObjectProperty rdf:about="#belongsToOrganization">
        <rdfs:label>Belongs To Organization</rdfs:label>
        <rdfs:domain rdf:resource="#Provider"/>
        <rdfs:range rdf:resource="#Organization"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasPrimaryProvider">
        <rdfs:label>Has Primary Provider</rdfs:label>
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="#Provider"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#locatedAt">
        <rdfs:label>Located At</rdfs:label>
        <rdfs:domain rdf:resource="#Organization"/>
        <rdfs:range rdf:resource="#Location"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#partOf">
        <rdfs:label>Part Of</rdfs:label>
        <rdfs:comment>Location hierarchy (bed partOf room partOf unit)</rdfs:comment>
        <rdfs:domain rdf:resource="#Location"/>
        <rdfs:range rdf:resource="#Location"/>
    </owl:ObjectProperty>

</rdf:RDF>
