<?xml version="1.0"?>
<rdf:RDF xmlns="http://aegis.health/ontology/clinical#"
     xml:base="http://aegis.health/ontology/clinical"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:core="http://aegis.health/ontology/core#"
     xmlns:fhir="http://hl7.org/fhir/">

    <owl:Ontology rdf:about="http://aegis.health/ontology/clinical">
        <rdfs:label>AEGIS Clinical Domain Ontology</rdfs:label>
        <rdfs:comment>Clinical entities: Encounter, Diagnosis, Procedure, Observation, Medication</rdfs:comment>
        <owl:versionInfo>0.1.0</owl:versionInfo>
        <owl:imports rdf:resource="http://aegis.health/ontology/core"/>
    </owl:Ontology>

    <!-- ============================================== -->
    <!-- CLASSES - Clinical Entities                    -->
    <!-- ============================================== -->

    <!-- Encounter -->
    <owl:Class rdf:about="#Encounter">
        <rdfs:label>Encounter</rdfs:label>
        <rdfs:comment>A healthcare encounter (admission, visit, etc.)</rdfs:comment>
    </owl:Class>

    <!-- Diagnosis (Condition) -->
    <owl:Class rdf:about="#Diagnosis">
        <rdfs:label>Diagnosis</rdfs:label>
        <rdfs:comment>A clinical diagnosis or condition (ICD-10 coded)</rdfs:comment>
    </owl:Class>

    <!-- Procedure -->
    <owl:Class rdf:about="#Procedure">
        <rdfs:label>Procedure</rdfs:label>
        <rdfs:comment>A clinical procedure (CPT/HCPCS coded)</rdfs:comment>
    </owl:Class>

    <!-- Observation -->
    <owl:Class rdf:about="#Observation">
        <rdfs:label>Observation</rdfs:label>
        <rdfs:comment>A clinical observation (vital signs, lab results, assessments)</rdfs:comment>
    </owl:Class>

    <!-- Medication -->
    <owl:Class rdf:about="#Medication">
        <rdfs:label>Medication</rdfs:label>
        <rdfs:comment>A medication order or administration</rdfs:comment>
    </owl:Class>

    <!-- AllergyIntolerance -->
    <owl:Class rdf:about="#AllergyIntolerance">
        <rdfs:label>Allergy/Intolerance</rdfs:label>
        <rdfs:comment>Patient allergy or intolerance record</rdfs:comment>
    </owl:Class>

    <!-- ClinicalNote -->
    <owl:Class rdf:about="#ClinicalNote">
        <rdfs:label>Clinical Note</rdfs:label>
        <rdfs:comment>Unstructured clinical documentation</rdfs:comment>
    </owl:Class>

    <!-- ============================================== -->
    <!-- DATA PROPERTIES - Encounter                    -->
    <!-- ============================================== -->

    <owl:DatatypeProperty rdf:about="#encounterType">
        <rdfs:label>Encounter Type</rdfs:label>
        <rdfs:domain rdf:resource="#Encounter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Values: inpatient, outpatient, emergency, observation</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#encounterClass">
        <rdfs:label>Encounter Class</rdfs:label>
        <rdfs:domain rdf:resource="#Encounter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>FHIR Encounter class codes: IMP, AMB, EMER, etc.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#encounterStatus">
        <rdfs:label>Encounter Status</rdfs:label>
        <rdfs:domain rdf:resource="#Encounter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Values: planned, in-progress, finished, cancelled</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#admitDate">
        <rdfs:label>Admit Date</rdfs:label>
        <rdfs:domain rdf:resource="#Encounter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#dischargeDate">
        <rdfs:label>Discharge Date</rdfs:label>
        <rdfs:domain rdf:resource="#Encounter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#admitSource">
        <rdfs:label>Admit Source</rdfs:label>
        <rdfs:domain rdf:resource="#Encounter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Where patient came from: emergency, transfer, physician referral</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#dischargeDisposition">
        <rdfs:label>Discharge Disposition</rdfs:label>
        <rdfs:domain rdf:resource="#Encounter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Where patient went: home, SNF, rehab, expired</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#lengthOfStay">
        <rdfs:label>Length of Stay (days)</rdfs:label>
        <rdfs:domain rdf:resource="#Encounter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <!-- ============================================== -->
    <!-- DATA PROPERTIES - Diagnosis                    -->
    <!-- ============================================== -->

    <owl:DatatypeProperty rdf:about="#icd10Code">
        <rdfs:label>ICD-10 Code</rdfs:label>
        <rdfs:domain rdf:resource="#Diagnosis"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#diagnosisDescription">
        <rdfs:label>Diagnosis Description</rdfs:label>
        <rdfs:domain rdf:resource="#Diagnosis"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#diagnosisType">
        <rdfs:label>Diagnosis Type</rdfs:label>
        <rdfs:domain rdf:resource="#Diagnosis"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Values: admitting, principal, secondary, comorbidity</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#diagnosisRank">
        <rdfs:label>Diagnosis Rank</rdfs:label>
        <rdfs:domain rdf:resource="#Diagnosis"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>1 = primary, 2+ = secondary</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#presentOnAdmission">
        <rdfs:label>Present On Admission</rdfs:label>
        <rdfs:domain rdf:resource="#Diagnosis"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>POA indicator: Y, N, U, W</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#onsetDate">
        <rdfs:label>Onset Date</rdfs:label>
        <rdfs:domain rdf:resource="#Diagnosis"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    </owl:DatatypeProperty>

    <!-- ============================================== -->
    <!-- DATA PROPERTIES - Procedure                    -->
    <!-- ============================================== -->

    <owl:DatatypeProperty rdf:about="#cptCode">
        <rdfs:label>CPT Code</rdfs:label>
        <rdfs:domain rdf:resource="#Procedure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hcpcsCode">
        <rdfs:label>HCPCS Code</rdfs:label>
        <rdfs:domain rdf:resource="#Procedure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#procedureDescription">
        <rdfs:label>Procedure Description</rdfs:label>
        <rdfs:domain rdf:resource="#Procedure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#procedureDate">
        <rdfs:label>Procedure Date</rdfs:label>
        <rdfs:domain rdf:resource="#Procedure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#procedureStatus">
        <rdfs:label>Procedure Status</rdfs:label>
        <rdfs:domain rdf:resource="#Procedure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Values: preparation, in-progress, completed, cancelled</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- ============================================== -->
    <!-- DATA PROPERTIES - Observation                  -->
    <!-- ============================================== -->

    <owl:DatatypeProperty rdf:about="#loincCode">
        <rdfs:label>LOINC Code</rdfs:label>
        <rdfs:domain rdf:resource="#Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#observationType">
        <rdfs:label>Observation Type</rdfs:label>
        <rdfs:domain rdf:resource="#Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Values: vital-signs, laboratory, assessment, imaging</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#observationValue">
        <rdfs:label>Observation Value</rdfs:label>
        <rdfs:domain rdf:resource="#Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#observationUnit">
        <rdfs:label>Observation Unit</rdfs:label>
        <rdfs:domain rdf:resource="#Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#observationDate">
        <rdfs:label>Observation Date</rdfs:label>
        <rdfs:domain rdf:resource="#Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#referenceRange">
        <rdfs:label>Reference Range</rdfs:label>
        <rdfs:domain rdf:resource="#Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#interpretation">
        <rdfs:label>Interpretation</rdfs:label>
        <rdfs:domain rdf:resource="#Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Values: normal, abnormal, critical, high, low</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- ============================================== -->
    <!-- DATA PROPERTIES - Medication                   -->
    <!-- ============================================== -->

    <owl:DatatypeProperty rdf:about="#ndcCode">
        <rdfs:label>NDC Code</rdfs:label>
        <rdfs:domain rdf:resource="#Medication"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#rxnormCode">
        <rdfs:label>RxNorm Code</rdfs:label>
        <rdfs:domain rdf:resource="#Medication"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#medicationName">
        <rdfs:label>Medication Name</rdfs:label>
        <rdfs:domain rdf:resource="#Medication"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#dosage">
        <rdfs:label>Dosage</rdfs:label>
        <rdfs:domain rdf:resource="#Medication"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#route">
        <rdfs:label>Route</rdfs:label>
        <rdfs:domain rdf:resource="#Medication"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Values: oral, IV, IM, subcutaneous, topical</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#frequency">
        <rdfs:label>Frequency</rdfs:label>
        <rdfs:domain rdf:resource="#Medication"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#medicationStatus">
        <rdfs:label>Medication Status</rdfs:label>
        <rdfs:domain rdf:resource="#Medication"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Values: active, completed, stopped, on-hold</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- ============================================== -->
    <!-- OBJECT PROPERTIES - Clinical Relationships     -->
    <!-- ============================================== -->

    <owl:ObjectProperty rdf:about="#hasEncounter">
        <rdfs:label>Has Encounter</rdfs:label>
        <rdfs:domain rdf:resource="http://aegis.health/ontology/core#Patient"/>
        <rdfs:range rdf:resource="#Encounter"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasDiagnosis">
        <rdfs:label>Has Diagnosis</rdfs:label>
        <rdfs:domain rdf:resource="#Encounter"/>
        <rdfs:range rdf:resource="#Diagnosis"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasProcedure">
        <rdfs:label>Has Procedure</rdfs:label>
        <rdfs:domain rdf:resource="#Encounter"/>
        <rdfs:range rdf:resource="#Procedure"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasObservation">
        <rdfs:label>Has Observation</rdfs:label>
        <rdfs:domain rdf:resource="#Encounter"/>
        <rdfs:range rdf:resource="#Observation"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasMedication">
        <rdfs:label>Has Medication</rdfs:label>
        <rdfs:domain rdf:resource="http://aegis.health/ontology/core#Patient"/>
        <rdfs:range rdf:resource="#Medication"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasAllergy">
        <rdfs:label>Has Allergy</rdfs:label>
        <rdfs:domain rdf:resource="http://aegis.health/ontology/core#Patient"/>
        <rdfs:range rdf:resource="#AllergyIntolerance"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#attendedBy">
        <rdfs:label>Attended By</rdfs:label>
        <rdfs:domain rdf:resource="#Encounter"/>
        <rdfs:range rdf:resource="http://aegis.health/ontology/core#Provider"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#performedBy">
        <rdfs:label>Performed By</rdfs:label>
        <rdfs:domain rdf:resource="#Procedure"/>
        <rdfs:range rdf:resource="http://aegis.health/ontology/core#Provider"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#locatedIn">
        <rdfs:label>Located In</rdfs:label>
        <rdfs:domain rdf:resource="#Encounter"/>
        <rdfs:range rdf:resource="http://aegis.health/ontology/core#Location"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#justifiedBy">
        <rdfs:label>Justified By</rdfs:label>
        <rdfs:comment>Procedure justified by diagnosis</rdfs:comment>
        <rdfs:domain rdf:resource="#Procedure"/>
        <rdfs:range rdf:resource="#Diagnosis"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#resultsIn">
        <rdfs:label>Results In</rdfs:label>
        <rdfs:comment>Procedure results in observation/finding</rdfs:comment>
        <rdfs:domain rdf:resource="#Procedure"/>
        <rdfs:range rdf:resource="#Observation"/>
    </owl:ObjectProperty>

</rdf:RDF>
